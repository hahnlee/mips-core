{
  "name": "mips-core",
  "version": "0.1.3",
  "description": "MIPS Assembler Simulator Inspired by MARS",
  "main": "build/mips-core.js",
  "types": "build/mips-core.d.ts",
  "scripts": {
    "prebuild": "rm -rf build && tsc --emitDeclarationOnly",
    "build": "webpack",
    "test": "nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Han Lee",
  "license": "MIT",
  "homepage": "https://github.com/sn0wle0pard/mips-core",
  "repository": {
    "type": "git",
    "url": "https://github.com/sn0wle0pard/mips-core.git"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.6",
    "awesome-typescript-loader": "^5.0.0",
    "coveralls": "^3.0.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.3.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/test/*.ts",
      "src/**/test/*.tsx",
      "src/**/index.ts",
      "src/lib.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "all": true,
    "sourceMap": true
  },
  "keywords": [
    "MIPS",
    "simulator"
  ]
}
